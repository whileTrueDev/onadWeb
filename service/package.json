{
  "name": "service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PORT=3001 node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "mysql": "^2.17.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "button-has-type": "null",
      "react/prefer-stateless-function": 1,
      "react/prop-types": [
        1,
        {
          "ignore": [
            "className",
            "children",
            "location",
            "params",
            "location*"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 80
  }
}
